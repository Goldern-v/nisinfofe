{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        }
      }
    ],
    "es2015",
    "stage-2"
  ],
  "plugins": [
    "transform-runtime",
    "transform-vue-jsx",
    "transform-class-properties"
    // [
    //   "component",
    //   {
    //     "libraryName": "ecrm-element-ui",
    //     "styleLibraryName": "theme-chalk"
    //   }
    // ],
    // [
    //   "import",
    //   {
    //     "libraryName": "view-design",
    //     "libraryDirectory": "src/components"
    //   }
    // ]
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  },
  "compact": false
}
