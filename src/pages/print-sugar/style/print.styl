#sugarPagePrint
  padding 0
  margin 0
  // background yellow
  overflow hidden
  .sugr-page
    margin 0 auto
    page-break-after always
    min-height 1000px
    // background red
  .noPrint
    display none
  .inPrint
    display block
@media print
  #sugarPagePrint
    .sugr-page
      padding 0
      margin 50px 0 0 !important
      box-shadow 0 0 0 0
      .page-con
        bottom 0
      .his-logo
        top 0
        left 0
@page {
  margin 0 10mm
}
